{
  "name": "next-boost",
  "version": "0.7.6",
  "description": "Add a cache layer for next.js SSR pages. Use stale-while-revalidate to boost the performance.",
  "main": "dist/handler.js",
  "bin": {
    "next-boost": "dist/next/server.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "rm -rf dist && npm run build && npm run test",
    "lint": "eslint src/*.ts test/*.ts",
    "test": "jest"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Rakuraku Jyo",
    "email": "jyo.rakuraku@gmail.com"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcovonly",
      "html"
    ],
    "sourceMap": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjyo/next-boost.git"
  },
  "homepage": "https://github.com/rjyo/next-boost",
  "license": "MIT",
  "dependencies": {
    "hybrid-disk-cache": "^0.1.5",
    "multee": "^0.2.2"
  },
  "peerDependencies": {
    "next": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.24",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "next",
    "nextjs",
    "ssr",
    "cache",
    "stale-while-revalidate",
    "react"
  ]
}
