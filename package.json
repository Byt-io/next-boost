{
  "name": "next-boost",
  "version": "0.5.12",
  "description": "Add a cache layer for next.js SSR pages. Use stale-while-revalidate to boost the performance.",
  "main": "dist/handler.js",
  "bin": {
    "next-boost": "dist/next/server.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "rm -rf dist && npm run build",
    "lint": "eslint src/*.ts test/*.ts",
    "test": "NODE_ENV=test mocha --require ts-node/register test/**/*.test.ts",
    "test-cov": "NODE_ENV=test nyc mocha test/*.test.ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Rakuraku Jyo",
    "email": "jyo.rakuraku@gmail.com"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcovonly",
      "html"
    ],
    "sourceMap": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjyo/next-boost.git"
  },
  "homepage": "https://github.com/rjyo/next-boost",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^6.0.1"
  },
  "peerDependencies": {
    "next": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "next",
    "nextjs",
    "ssr",
    "cache",
    "stale-while-revalidate",
    "react"
  ]
}
