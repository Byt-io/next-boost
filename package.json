{
  "name": "next-boost",
  "version": "0.7.2",
  "description": "Add a cache layer for next.js SSR pages. Use stale-while-revalidate to boost the performance.",
  "main": "dist/handler.js",
  "bin": {
    "next-boost": "dist/next/server.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "rm -rf dist && npm run build",
    "lint": "eslint src/*.ts test/*.ts",
    "test": "mocha",
    "test-cov": "nyc mocha"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Rakuraku Jyo",
    "email": "jyo.rakuraku@gmail.com"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcovonly",
      "html"
    ],
    "sourceMap": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjyo/next-boost.git"
  },
  "homepage": "https://github.com/rjyo/next-boost",
  "license": "MIT",
  "dependencies": {
    "@rjyo/mock-http": "^1.1.1",
    "hybrid-disk-cache": "^0.1.2",
    "multee": "^0.1.2"
  },
  "peerDependencies": {
    "next": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@types/supertest": "^2.0.9",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "next",
    "nextjs",
    "ssr",
    "cache",
    "stale-while-revalidate",
    "react"
  ]
}
